#
# .gitconfig
#
[core]
    editor = vim
    excludesfile = ~/.gitignore
    trustctime = false
[commit]
    template = ~/.gitmessage
[include]
    path = ~/.gitconfig_local
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    grep = auto
[alias]
    st  = status
    ad  = add
    ai  = add -i
    cm  = commit -m
    cma = commit -a -m
    co  = checkout
    df  = diff
    ft  = fetch
    mg  = merge
    ps  = push
    br  = branch
    remotes = remote -v
    tr = log --graph --branches --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
[push]
default = matching
[http]
    proxy = 
[init]
	defaultBranch = main
