            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500
                },
                "rules": [
                    {
                        "description": "Xcode で右shift２連打を Cmd+Shift+o に割り当てる",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "isRightShiftPressed",
                                        "type": "variable_if",
                                        "value": 1
                                    },
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.apple\\.dt\\.Xcode$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "right_shift"
                                },
                                "to": [
                                    {
                                        "key_code": "o",
                                        "modifiers": [
                                            "left_command",
                                            "right_shift"
                                        ]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_shift"
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "isRightShiftPressed",
                                            "value": 1
                                        }
                                    },
                                    {
                                        "key_code": "right_shift"
                                    }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "isRightShiftPressed",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
                                        {
                                            "set_variable": {
                                                "name": "isRightShiftPressed",
                                                "value": 0
                                            }
                                        }
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
