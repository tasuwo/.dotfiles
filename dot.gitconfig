#
# .gitconfig
#
[core]
    editor = emacs
    excludesfile = ~/.gitignore
[include]
    path = ~/.gitconfig_local
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    grep = auto
[alias]
    st = status
    cm = commit
    co = checkout
    df = diff
    ft = fetch
    mg = merge
    remotes = remote -v